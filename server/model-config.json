{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "users": {
      "type": "hasMany",
      "model": "UserInfo",
      "foreignKey": "roleId",
      "through": "RoleMapping"
    }
  },
  "UserInfo": {
    "dataSource": "db",
    "public": true
  },
  "Department": {
    "dataSource": "db",
    "public": true
  },
  "Site": {
    "dataSource": "db",
    "public": true
  },
  "PurchaseStatus": {
    "dataSource": "db",
    "public": true
  },
  "PurchaseOrder": {
    "dataSource": "db",
    "public": true
  },
  "Inventory": {
    "dataSource": "db",
    "public": true
  },
  "PurchaseOrderInfo": {
    "dataSource": "db",
    "public": true
  },
  "Vendor": {
    "dataSource": "db",
    "public": true
  }
}
